<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Geico Automation Script " parallel="false">
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="saucelabs"/>

    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="62"/>
    <parameter name="url" value="http://www.geico.com"/>

    <test name = "Geico Homepage Test">
        <classes>
            <class name="homepagetest.SearchFunctionalityTest"/>
            <class name="homepagetest.FeedbackTest"/>
            <class name="homepagetest.LandingPageBasicTest"/>
            <class name="homepagetest.QuotesOnProductsTest"/>
            <class name="homepagetest.YourPolicyDDTest"/>
            <class name="homepagetest.InformationDDTest"/>
            <class name="homepagetest.InsuranceDDTest"/>
            <class name="homepagetest.LoginFunctionalityTest"/>
            <class name="footerpagetest.MediaLinksTest"/>
            <class name="footerpagetest.WebsiteLinksTest"/>
            <class name="autoinsurancequotetest.YourInfoPageTest"/>
            <class name="autoinsurancequotetest.YourAddressPageTest"/>
            <class name="autoinsurancequotetest.VehicleDetailsPageTest"/>
            <class name="autoinsurancequotetest.DriverInformationPageTest"/>
            <class name="homeownersinsurancequotetest.CustomerInformationPageTest"/>
            <class name="datadriventest.SearchTestWithExcelReader"/>
            <class name="datadriventest.SearchTestWithGoogleSheetApi"/>
            <class name="datadriventest.SearchTestWithEnterFromDatabase"/>
            <class name="datadriventest.LoginTestWithDataBase"/>
            <class name="datadriventest.LoginTestWithExcel"/>
            <class name="datadriventest.LoginTestWithGoogleSheetApi"/>
            <class name="keyworddriventest.FeatureTest"/>

        </classes>
    </test>
</suite>