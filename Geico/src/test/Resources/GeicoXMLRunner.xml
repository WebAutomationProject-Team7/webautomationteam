<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Geico Automation Script " parallel="false">
    <!--<parameter name="useCloudEnv" value="false"/>
   <parameter name="cloudEnvName" value="browserstack"/>-->

    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="62.0"/>
    <parameter name="url" value="http://www.geico.com"/>

    <test name = "Geico Homepage Test">
        <classes>
            <class name="homePageTest.SearchFunctionalityTest"/>
            <class name="homePageTest.FeedbackTest"/>
            <class name="homePageTest.LandingPageBasicTest"/>
        </classes>
    </test>
</suite>